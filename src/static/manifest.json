{
  "manifest_version": 2,
  "name": "Slicon",
  "description": "Set icon to your slack channel, and make more accessible for your favorite channel!",
  "version": "0.1.1",
  "content_scripts": [{
    "matches": [
      "https://*.slack.com/*"
    ],
    "js": [
      "content_script.js"
    ],
    "css": [
      "main.css"
    ],
    "run_at": "document_start"
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "https://*.slack.com/*",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ]
}
